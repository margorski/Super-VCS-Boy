var count=0x80,camera_y=0x81, y_pos=0x82,sprite_byte=0x83

// x - pos
func set_pos_x {
	nocross {	// make sure following code doesn't cross page boundary
		wsync { a-15 }>=0 a<< a<< a<< a<< a^0x70 hp0=a *5 rp0=a	wsync hmove=a	// sprite position trick
	}
}

main {
	init
	cpf=a=0x34
	cp0=a=0x44
	//cbg=a=0x30
	camera_y=a=0	
	y_pos=a=30
	{
		sync1
			x=30
			set_pos_x
		sync2			
			count=a=0
			x=0
			y=192		
		sync3
			{
				a=y a>> a>> a>> c- a+camera_y x=a 
				// 6 wolnych cykli aby ustawiÄ‡ sprite, impossibru
				*6
				wsync				
				pf0=a=pf0_0,x
				pf1=a=pf1_0,x
				pf2=a=pf2_0,x
				
				*5

				pf0=a=pf0_1,x
				pf1=a=pf1_1,x
				pf2=a=pf2_1,x
				y--
			}!=
			pf0=pf1=pf2=a=0
	} always	// loop forever
}

